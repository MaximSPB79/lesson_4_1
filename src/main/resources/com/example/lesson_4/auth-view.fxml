<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="281.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lesson_4.controller.AuthController">
    <children>
        <TextField fx:id="loginField" layoutX="161.0" layoutY="60.0" prefHeight="51.0" prefWidth="298.0">
            <font>
                <Font size="24.0"/>
            </font>
        </TextField>
        <PasswordField fx:id="passwordField" layoutX="161.0" layoutY="133.0" onAction="#checkAuth">
            <font>
                <Font size="24.0"/>
            </font>
        </PasswordField>
        <Label layoutX="28.0" layoutY="68.0" text="Login">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label layoutX="28.0" layoutY="148.0" text="Password">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Button layoutX="184.0" layoutY="204.0" mnemonicParsing="false" onAction="#checkAuth" text="Sign in">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
